.mux-taskbar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 var(--muxGapSize);
  background-color: var(--muxBarColor);

  .mux-taskbar-pinned-programs {
    display: flex;
    gap: 10px;
    list-style: none;
    padding-inline-start: 0;

    img {
      width: 32px;
      height: 32px;
      cursor: pointer;
    }
  }
}