.mux-program-window {
  border: 5px solid red;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  grid-template-rows: 60px 1fr;
  width: 100%;
  height: 100%;

  .mux-program-window-content {
    width: 100%;
    height: 100%;
    overflow: auto;
    box-sizing: border-box;
  }
}